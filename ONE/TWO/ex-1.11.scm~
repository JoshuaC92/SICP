#lang sicp

(define (f-recursive n)
  (if (< n 3) 
      n-2
      (+ (f-recursive (- n 1))
         (* 2 (f-recursive (- n 2)))
         (* 3 (f-recursive (- n 3))))))

(define (f-iterative n)
  (if (< n 3)
      n
      (f-iter 2 1 0 n)))

(define (f-iter x y z n-counter)
  (if (< n-counter 3)
      x
      (f-iter (+ x (* 2 y) (* 3 z))
              x
              y
              (- n-counter 1))))

